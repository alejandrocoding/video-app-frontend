{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "../dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "paths": {
      "@app/*": [
        "app/*"
      ],
      "@environments/*": [
        "environments/*"
      ],
      "@layout/*": [
        "app/_layout/*"
      ],
      "@core/*": [
        "app/_core/*"
      ],
      "@common/*": [
        "app/_common/*"
      ],
      "@shared/*": [
        "app/_shared/*"
      ],
      "@components/*": [
        "app/_common/components/*"
      ],
      "@services/*": [
        "app/_common/services/*"
      ],
      "@interfaces/*": [
        "app/_shared/interfaces/*"
      ],
      "@models/*": [
        "app/_shared/models/*"
      ],
      "@enums/*": [
        "app/_shared/enums/*"
      ],
      "@validators/*": [
        "app/_shared/validators/*"
      ],
    },
    "typeRoots": [
      "../node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  }
}